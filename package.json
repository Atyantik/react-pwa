    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.19.1",
    "workbox-sw": "^2.1.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --exec babel-node --watch ./src/server.js --watch ./src/client.js --watch ./src/core --watch ./webpack ./src/server.js",
    "lint": "eslint --ignore-path .gitignore src webpack",
    "build": "cross-env NODE_ENV=production webpack -p --config ./webpack/prod.client.babel.js && cross-env NODE_ENV=production webpack -p --config ./webpack/prod.server.babel.js",
    "test": "npm run lint && cross-env NODE_ENV=test mocha --require babel-core/register \"src/**/*.test.js\"",

    "docker:dev:image": "docker build -t react-pwa -f docker/Dockerfile .",
    "docker:dev:start": "docker run -it -v \"$PWD/src\":\"/app/src\" -p 3003:3003 react-pwa",
    "docker:dev:list": "docker run -it -v \"$PWD/src\":\"/app/src\" -p 3003:3003 react-pwa yarn lint",
    "docker:dev:test": "docker run -it -v \"$PWD/src\":\"/app/src\" -p 3003:3003 react-pwa yarn test",
    "docker:prod:build": "docker run -it -v \"$PWD/src\":\"/app/src\" -v \"$PWD/dist\":\"/app/dist\" react-pwa yarn build",
    "docker:prod:image": "npm run docker:dev:image && npm run docker:prod:build && docker build -t react-pwa-prod -f docker/prod/Dockerfile .",
    "docker:prod:start": "docker run -it -p 3003:3003 react-pwa-prod",

    "postinstall": "opencollective postinstall"
  },
  "keywords": [
    "es7",
    "nodejs"
  ],
  "author": "Tirth Bodawala",
  "license": "ISC",
  "dependencies": {
    "opencollective": "^1.0.3"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/react-pwa",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  }
}
