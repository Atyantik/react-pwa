{
  "name": "@reactpwa/core",
  "version": "1.0.50-alpha.14",
  "description": "Core of ReactPWA",
  "type": "module",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "prettier --write . && eslint . --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "typings": "typings.d.ts",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "files": [
    "lib/*",
    "assets/*",
    "index.js",
    "router.d.ts",
    "router.js",
    "typings.d.ts",
    "reactpwa.js"
  ],
  "exports": {
    ".": {
      "types": "./typings.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    },
    "./start": {
      "types": "./lib/node/start.d.ts",
      "import": "./lib/node/start.js",
      "require": "./lib/node/start.cjs"
    },
    "./build": {
      "types": "./lib/node/build.d.ts",
      "import": "./lib/node/build.js",
      "require": "./lib/node/build.cjs"
    },
    "./webpack": {
      "types": "./lib/webpack.d.ts",
      "import": "./lib/webpack.js",
      "require": "./lib/webpack.cjs"
    },
    "./router": {
      "types": "./router.d.ts",
      "import": "./lib/router.js",
      "require": "./lib/router.cjs"
    },
    "./assets/*": "./assets/*",
    "./package.json": "./package.json"
  },
  "keywords": [
    "react",
    "react-dom",
    "pwa",
    "progressive",
    "reactpwa"
  ],
  "author": "Tirth Bodawala <tirthbodawala@atyantik.com>",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.3",
    "bowser": "^2.11.0",
    "compression": "^1.7.4",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "express": "^5.0.0-beta.1",
    "fs-extra": "^11.1.1",
    "is-object": "^1.0.2",
    "isbot": "^3.6.13",
    "mini-css-extract-plugin": "^2.7.6",
    "node-sass": "^9.0.0",
    "postcss-loader": "^7.3.3",
    "react-cookie": "^4.1.1",
    "react-error-boundary": "^4.0.10",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.14.2",
    "sass": "^1.64.1",
    "sass-embedded": "^1.64.1",
    "sass-loader": "^13.3.2",
    "serialize-javascript": "^6.0.1",
    "style-loader": "^3.3.3",
    "universal-cookie": "^4.0.4",
    "webpack": "^5.88.2",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-hot-middleware": "^2.25.4",
    "webpack-node-externals": "^3.0.0",
    "workbox-webpack-plugin": "^7.0.0"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/fs-extra": "^11.0.1",
    "@types/is-object": "^1.0.2",
    "@types/node-sass": "^4.11.3",
    "@types/require-from-string": "^1.2.1",
    "@types/serialize-javascript": "^5.0.2",
    "@types/webpack-hot-middleware": "^2.25.6",
    "@types/webpack-node-externals": "^3.0.0",
    "eslint": "^8.45.0",
    "eslint-config-reactpwa": "^1.0.50-alpha.14",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^7.1.0"
  },
  "gitHead": "1b9ab957cac25218179b12f2ddc727fabb06f000"
}
