{
  "name": "@reactpwa/core",
  "version": "1.0.36-alpha.0",
  "description": "Core of ReactPWA",
  "type": "module",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "prettier --write . && eslint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "typings": "./lib",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "files": [
    "lib/*",
    "assets/*",
    "reactpwa.js"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    },
    "./start": {
      "types": "./lib/node/start.d.ts",
      "import": "./lib/node/start.js",
      "require": "./lib/node/start.cjs"
    },
    "./build": {
      "types": "./lib/node/build.d.ts",
      "import": "./lib/node/build.js",
      "require": "./lib/node/build.cjs"
    },
    "./webpack": {
      "types": "./lib/webpack.d.ts",
      "import": "./lib/webpack.js",
      "require": "./lib/webpack.cjs"
    },
    "./router": {
      "types": "./lib/router.d.ts",
      "import": "./lib/router.js",
      "require": "./lib/router.cjs"
    },
    "./assets/*": "./assets/*",
    "./package.json": "./package.json"
  },
  "keywords": [
    "react",
    "react-dom",
    "pwa",
    "progressive",
    "reactpwa"
  ],
  "author": "Tirth Bodawala <tirthbodawala@atyantik.com>",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@fastify/compress": "^6.2.0",
    "@fastify/cookie": "^8.3.0",
    "@fastify/express": "^2.1.0",
    "@fastify/static": "^6.5.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "bowser": "^2.11.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.2",
    "fastify": "^4.10.2",
    "fs-extra": "^10.1.0",
    "http-errors": "^2.0.0",
    "is-object": "^1.0.2",
    "isbot": "^3.6.5",
    "mini-css-extract-plugin": "^2.7.0",
    "postcss-loader": "^7.0.1",
    "react-cookie": "^4.1.1",
    "react-error-boundary": "^3.1.4",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.4.3",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "serialize-javascript": "^6.0.0",
    "style-loader": "^3.3.1",
    "universal-cookie": "^4.0.4",
    "webpack": "^5.75.0",
    "webpack-dev-middleware": "^6.0.0",
    "webpack-hot-middleware": "^2.25.3",
    "webpack-node-externals": "^3.0.0",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/http-errors": "^2.0.1",
    "@types/is-object": "^1.0.2",
    "@types/require-from-string": "^1.2.1",
    "@types/serialize-javascript": "^5.0.2",
    "@types/webpack-hot-middleware": "^2.25.6",
    "@types/webpack-node-externals": "^2.5.3",
    "eslint": "^8.28.0",
    "eslint-config-reactpwa": "^1.0.35-alpha.0",
    "prettier": "^2.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^6.5.0"
  },
  "gitHead": "1b9ab957cac25218179b12f2ddc727fabb06f000"
}
