{
  "name": "@reactpwa/core",
  "version": "1.0.2-alpha.0",
  "description": "Core of ReactPWA",
  "type": "module",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "prettier --write . && eslint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "typings": "./lib",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "files": [
    "lib/*",
    "assets/*",
    "reactpwa.js"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    },
    "./start": {
      "types": "./lib/node/start.d.ts",
      "import": "./lib/node/start.js",
      "require": "./lib/node/start.cjs"
    },
    "./build": {
      "types": "./lib/node/build.d.ts",
      "import": "./lib/node/build.js",
      "require": "./lib/node/build.cjs"
    },
    "./webpack": {
      "types": "./lib/webpack.d.ts",
      "import": "./lib/webpack.js",
      "require": "./lib/webpack.cjs"
    },
    "./assets/*": "./assets/*"
  },
  "keywords": [
    "react",
    "react-dom",
    "pwa",
    "progressive",
    "reactpwa"
  ],
  "author": "Tirth Bodawala <tirthbodawala@atyantik.com>",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@fastify/compress": "^6.1.1",
    "@fastify/cookie": "^8.3.0",
    "@fastify/express": "^2.0.2",
    "@fastify/static": "^6.5.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "babel-plugin-lodash": "^3.3.4",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "fastify": "^4.7.0",
    "http-errors": "^2.0.0",
    "is-object": "^1.0.2",
    "isbot": "^3.6.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss-loader": "^7.0.1",
    "react-cookie": "^4.1.1",
    "react-error-boundary": "^3.1.4",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.4.2",
    "sass": "^1.55.0",
    "sass-loader": "^13.0.2",
    "serialize-javascript": "^6.0.0",
    "style-loader": "^3.3.1",
    "universal-cookie": "^4.0.4",
    "webpack": "^5.74.0",
    "webpack-dev-middleware": "^5.3.3",
    "webpack-hot-middleware": "^2.25.2",
    "webpack-node-externals": "^3.0.0",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "devDependencies": {
    "@types/http-errors": "^1.8.2",
    "@types/is-object": "^1.0.2",
    "@types/require-from-string": "^1.2.1",
    "@types/serialize-javascript": "^5.0.2",
    "@types/webpack-hot-middleware": "^2.25.6",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "eslint": "^8.24.0",
    "eslint-config-reactpwa": "^1.0.2-alpha.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^6.2.3"
  },
  "gitHead": "1b9ab957cac25218179b12f2ddc727fabb06f000"
}
