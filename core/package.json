    "setup": "babel-node ./setup.js",
    "start": "cross-env NODE_ENV=development nodemon --exec babel-node --watch ../src/server.js --watch ./src --watch ./webpack ../src/server.js",
    "lint": "eslint -c ../.eslintrc --ignore-path ../.gitignore ../src",
    "lint:core": "eslint -c ./.eslintrc --ignore-path ../.gitignore ./src ./webpack",
    "build": "cross-env NODE_ENV=production webpack -p --config ./webpack/prod.client.babel.js && cross-env NODE_ENV=production webpack -p --config ./webpack/prod.server.babel.js",
